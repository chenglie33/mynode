<!-- Start: Content -->
<div class="container-fluid content">
    <div class="row">

        <!-- Sidebar -->
        <div class="sidebar">
            <div class="sidebar-collapse">
                <!-- Sidebar Header Logo-->
                <div class="sidebar-header">
                    <img src="assets/img/logo.png" class="img-responsive" alt="" />
                </div>
                <!-- Sidebar Menu-->
                <div class="sidebar-menu">
                    <nav id="menu" class="nav-main" role="navigation">
                        <ul class="nav nav-sidebar">
                            <div class="panel-body text-center">
                                <div class="bk-avatar">
                                    <img src="assets/img/avatar.jpg" class="img-circle bk-img-60" alt="" />
                                </div>
                                <div class="bk-padding-top-10">
                                    <i class="fa fa-circle text-success"></i> <small><%=name%></small>
                                </div>
                            </div>
                            <div class="divider2"></div>
                            <li class="active">
                                <a href="/backstage/">
                                    <i class="fa fa-laptop" aria-hidden="true"></i><span>案例</span>
                                </a>
                            </li>

                            <li class="nav-parent">
                                <a>
                                    <i class="fa fa-copy" aria-hidden="true"></i><span>笔记</span>
                                </a>
                                <ul class="nav nav-children">
                                    <li><a href="#/articless/Java"><span class="text"> Java</span></a></li>
                                    <li><a href="#/articless/NodeJS"><span class="text"> NodeJS</span></a></li>
                                    <li><a href="#/articless/JavaScript"><span class="text"> JavaScript</span></a></li>
                                    <li><a href="#/articless/Other"><span class="text"> Other </span></a></li>
                                </ul>
                            </li>


                        </ul>
                    </nav>
                </div>
                <!-- End Sidebar Menu-->
            </div>
            <!-- Sidebar Footer-->
            <div class="sidebar-footer">
                <ul class="sidebar-terms">
                    <li><a href="index.html#">Terms</a></li>
                    <li><a href="index.html#">Privacy</a></li>
                    <li><a href="index.html#">Help</a></li>
                    <li><a href="index.html#">About</a></li>
                </ul>
                <div class="copyright text-center">
                    <small>Nadhif <i class="fa fa-coffee"></i> Collect from <a href="http://bootstrap.cn/" title="网页模板" target="_blank">网页模板</a> - More Templates <a href="http://bootstrap.cn/" target="_blank" title="Bootstrap学习网">Bootstrap学习网</a></small>
                </div>
            </div>
            <!-- End Sidebar Footer-->
        </div>
        <!-- End Sidebar -->

        <!-- Main Page -->
        <div class="main " ng-cloak>
            <div ui-view></div>
        </div>
        <!-- End Main Page -->

        <!-- Usage -->
        <div id="usage">
            <ul>
                <li>
                    <div class="title">Memory</div>
                    <div class="bar">
                        <div class="progress progress-md  progress-striped active">
                            <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="50" aria-valuemin="0" aria-valuemax="100" style="width: 50%"></div>
                        </div>
                    </div>
                    <div class="desc">4GB of 8GB</div>
                </li>
                <li>
                    <div class="title">HDD</div>
                    <div class="bar">
                        <div class="progress progress-md  progress-striped active">
                            <div class="progress-bar progress-bar-primary" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%"></div>
                        </div>
                    </div>
                    <div class="desc">250GB of 1TB</div>
                </li>
                <li>
                    <div class="title">SSD</div>
                    <div class="bar">
                        <div class="progress progress-md  progress-striped active">
                            <div class="progress-bar progress-bar-warning" role="progressbar" aria-valuenow="70" aria-valuemin="0" aria-valuemax="100" style="width: 70%"></div>
                        </div>
                    </div>
                    <div class="desc">700GB of 1TB</div>
                </li>
                <li>
                    <div class="title">Bandwidth</div>
                    <div class="bar">
                        <div class="progress progress-md  progress-striped active">
                            <div class="progress-bar progress-bar-danger" role="progressbar" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100" style="width: 90%"></div>
                        </div>
                    </div>

                </li>
            </ul>
        </div>
        <!-- End Usage -->

    </div>
</div><!--/container-->


<!-- Modal Dialog -->
<div class="modal fade" id="myModal">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title bk-fg-primary" id="modal-title">Modal title</h4>
            </div>
            <div class="modal-body">
                <p class="bk-fg-danger" id="modal-content">Here settings can be configured...</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary" id="btn-auto">Save changes</button>
            </div>
        </div>
    </div>
</div><!-- End Modal Dialog -->

<!-- alert Dialog -->
<div class="modal fade bs-example-modal-sm" id="alert" tabindex="-1" role="dialog" aria-labelledby="mySmallModalLabel" aria-hidden="false">
    <div class="modal-dialog modal-sm">
        <div class="modal-content" >
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title" id="myModalLabel">Modal title</h4>
            </div>
            <div class="modal-body" id="alert_content">
                ...
            </div>

        </div>
    </div>
</div><!-- alert Dialog -->

